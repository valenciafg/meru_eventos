{
  "name": "meru_eventos",
  "productName": "calendario-meru",
  "version": "1.1.0",
  "description": "Calendario de Eventos Merú",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=dist/images/logo.ico --prune=true --out=release-builds --win32metadata.CompanyName=\"Hotel Plaza Merú\" --win32metadata.FileDescription=\"Calendarios de Eventos Merú\" --win32metadata.ProductName=\"Calendario Plaza Merú\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/images/hotel_logof.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valenciafg/meru_eventos.git"
  },
  "author": {
    "name": "Victor Valencia",
    "email": "valencia6x@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valenciafg/meru_eventos/issues"
  },
  "homepage": "https://github.com/valenciafg/meru_eventos",
  "devDependencies": {
    "asset-builder": "^1.1.0",
    "browser-sync": "^2.17.5",
    "del": "^2.2.2",
    "electron": "^1.6.2",
    "electron-builder": "^17.1.2",
    "electron-packager": "^8.5.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-changed": "^1.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "^0.3.1",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-jshint": "^2.0.2",
    "gulp-less": "^3.1.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "jshint": "^2.9.4",
    "jshint-stylish": "^2.2.1",
    "lazypipe": "^1.0.1",
    "merge-stream": "^1.0.0",
    "minimist": "^1.2.0",
    "run-sequence": "^1.2.2",
    "wiredep": "^4.0.0"
  }
